<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Receipt</servlet-name>
        <servlet-class>Class.Receipt</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PdfServlet</servlet-name>
        <servlet-class>Class.PdfServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Receipt</servlet-name>
        <url-pattern>/Receipt</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PdfServlet</servlet-name>
        <url-pattern>/PdfServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Login</servlet-name>
        <jsp-file>/Entrance/login.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Entrance/Sign-in</url-pattern>
    </servlet-mapping>
<!--    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Entrance/*</url-pattern>
    </servlet-mapping>-->
    <!--log in IC-->
    <servlet>
        <servlet-name>Login_IC</servlet-name>
        <jsp-file>/Entrance/login_ic.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login_IC</servlet-name>
        <url-pattern>/Entrance/ReSign-in</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Forgot_pwd</servlet-name>
        <jsp-file>/Entrance/forgot-pwd.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>Forgot_pwd</servlet-name>
        <url-pattern>/Entrance/Forgot_Password</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Dashboard</servlet-name>
        <jsp-file>/Entrance/dashboard.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>Dashboard</servlet-name>
        <url-pattern>/Entrance/Home</url-pattern>
    </servlet-mapping>
    
    <!--mapping for system admin start-->
    <servlet>
        <servlet-name>adm_lookup</servlet-name>
        <jsp-file>/SystemAdmin/HIS010001.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>adm_lookup</servlet-name>
        <url-pattern>/SystemAdmin/Lookup</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>adm_users</servlet-name>
        <jsp-file>/SystemAdmin/HIS010002.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>adm_users</servlet-name>
        <url-pattern>/SystemAdmin/UserManagement</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>adm_queue</servlet-name>
        <jsp-file>/SystemAdmin/HIS010003.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>adm_queue</servlet-name>
        <url-pattern>/SystemAdmin/QueueManagement</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>adm_hfc</servlet-name>
        <jsp-file>/SystemAdmin/HIS010004.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>adm_hfc</servlet-name>
        <url-pattern>/SystemAdmin/HealthFacilityManagement</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>adm_setting</servlet-name>
        <jsp-file>/SystemAdmin/HIS010006.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>adm_setting</servlet-name>
        <url-pattern>/SystemAdmin/Setting</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>adm_system</servlet-name>
        <jsp-file>/SystemAdmin/HIS010005.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>adm_system</servlet-name>
        <url-pattern>/SystemAdmin/System</url-pattern>
    </servlet-mapping>
    <!--mapping for system admin end-->
    
    <session-config>
        <session-timeout>
            -1
        </session-timeout>
    </session-config>
</web-app>
